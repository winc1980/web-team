---
const { title, bgImg, imgClass, items } = Astro.props
---

<div class="web-team__mv">
  <div class={'web-team__mv__bg ' + imgClass}><img src={bgImg} alt="メインビジュアル"/></div>
  <ul>
    {
      items.map((i) => {
        return (
          <li>
            <a href={i.link}> {i.text}</a>
          </li>
        )
      })
    }
  </ul>
  <h1>{title}</h1>
</div>
